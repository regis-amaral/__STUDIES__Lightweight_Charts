{
  "name": "meu-servidor-websocket",
  "version": "1.0.0",
  "description": "Servidor WebSocket para ler dados de mercado da Binance e atualizar clientes web",
  "main": "dist/App.js",
  "scripts": {
    "dev": "npm run development",
    "development": "node_modules/.bin/cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "prod": "npm run production",
    "production": "node_modules/.bin/cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "start": "tsc && npm run dev && node dist/back/App.js"
  },
  "author": "Seu Nome",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.8",
    "cross-env": "^7.0.3",
    "express": "^4.19.2",
    "http-server": "^14.1.1",
    "laravel-mix": "^6.0.49",
    "lightweight-charts": "^4.1.3",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.75.0",
    "sass-loader": "^12.6.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "websocket": "^1.0.34",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5"
  }
}
